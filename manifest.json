{
  "manifest_version": 3,
  "name": "Simple To-Do Extension",
  "version": "1.0",
  "description": "A simple new tab to-do list.",
  "permissions": ["storage"],
  "background": { "service_worker": "background.js" },
  "action": {
    "default_popup": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}